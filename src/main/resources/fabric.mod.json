{
  "schemaVersion": 1,
  "id": "refabslab",
  "version": "${version}",
  "name": "Refabricated Slabs",
  "description": "Mix and match slabs as you want!",
  "authors": [
    "CebbyS"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/refabricated-slabs",
    "sources": ""
  },
  "license": "CC0-1.0",
  "icon": "assets/refabslab/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "lv.cebbys.mcmods.refabslab.Refabslab"
    ],
    "client": [
      "lv.cebbys.mcmods.refabslab.Refabslab"
    ],
    "cardinal-components-chunk": [
      "lv.cebbys.mcmods.refabslab.content.RefabslabComponents"
    ]
  },
  "custom": {
    "cardinal-components": [
      "refabslab:double_slabs"
    ]
  },
  "mixins": [
    "refabslab.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.0",
    "fabric": ">=0.56.0",
    "minecraft": "${minecraft_dependency}",
    "celib": "${celib_dependency}",
    "respro": "${respro_dependency}",
    "cardinal-components-base": "${cardinal_api_dependency}",
    "cardinal-components-chunk": "${cardinal_api_dependency}"
  }
